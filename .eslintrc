{
  "extends": ["react-app", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": "off",
    "react/prop-types": "error",
    "lines-between-class-members": "error"
  },
  "plugins": ["prettier"]
}
